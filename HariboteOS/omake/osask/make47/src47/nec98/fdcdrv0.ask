//	EAX += 1024 - 512 - 1;
//	EAX &= 0xfffffc00; // KBíPà Ç…êÿÇËè„Ç∞
//	EAX += 8191;
//	(unsigned int) EAX /= 8192;
//	AL += 2;
//	[DS:0x10003f] = AL;
	POP(ESI);
	POP(DS);

	/* ÉâÉCÉgÉLÉÉÉbÉVÉÖÇê›íË */
	EAX = 0;
	ECX = 360;
	do {
	//	work->WBFS[ECX - 4] = EAX;
		[ECX - 4 + work->WBFS] = EAX;
		ECX -= 4;
	} while (!= 0);
	(char) work->WBFS[0] = 0x01; /* bit0 */
	ECX = [FS:EBX + 12];
	EDX = 16;
	ECX += 1023 - 512;
	ECX /= 1024;
	do {
		BTS((int) work->WBFS[0], EDX);
		EDX++;
		ECX--;
	} while (!= 0);

	asmout("JMP FDC_format98");
