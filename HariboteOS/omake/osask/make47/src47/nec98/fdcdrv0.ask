void FDC_createfile()
// ECX:çÏã∆íÜÇÃÉVÉOÉiÉã
// EDX:äÆóπÇÃÉVÉOÉiÉã
// GS:ESI ÉtÉ@ÉCÉãñº(11bytes)
{
	unsigned int tapi_sel == 12 * 8;
	FDC_WORK *work == DS:0;

	if (work->dosload_phase != 0) {
		INT(0x03);
	}

	work->dosload_sigtmp = ECX;
	work->dosload_sigfin = EDX;

	// àÀóäÇ≥ÇÍÇΩílÇworkÇ…ï€ë∂ÇµÇƒÇ®Ç≠
	ECX = 32;
	LEA(EBX, work->dostmp[0]);
	do {
		AL = [GS:ESI];
		ESI++;
		[DS:EBX] = AL;
		EBX++;
		ECX--;
	} while (!= 0);

	ready_track01();

	// ãÛÇ´ÉGÉìÉgÉäÇíTÇ∑
//	if ((unsigned) (char) work->dosload_name[0] == 0)
//		goto pass98;

	EAX = 1 * 8;
	PUSH(ES);
	ES = EAX;
	EDI = ROOT_ENTRY0;
	do {
		AL = [ES:EDI + 0];
		if (AL == 0xe5)
			goto find;
		if (AL == 0)
			goto find;
		EDI += 32;
	} while ((unsigned) EDI < ROOT_ENTRY1);
	POP(ES);
	// ãÛÇ´Ç™å©Ç¬Ç©ÇÁÇ»Ç©Ç¡ÇΩÇÃÇ≈âΩÇ‡ÇµÇ»Ç¢Ç≈ãAÇÈ
	goto send_sigfin;
//	return;
//	INT(0x03);
find:
	if (AL == 0) {
		if ((unsigned) EDI < ROOT_ENTRY1 - 32)
			(char) [ES:EDI + 32] = 0x00;
	}
	PUSH(EDI);
	ECX = 32 / 4;
	LEA(EBX, work->dostmp[0]);
	do {
		EAX = [DS:EBX];
		EBX += 4;
		[ES:EDI] = EAX;
		EDI += 4;
		ECX--;
	} while (!= 0);
	POP(EDI);
	POP(ES);
	EDI -= TRACK0_ADDR0;
	EDI /= SECTOR_BYTES;
	BTS(work->WBFS[0], EDI);
	check_WBF();
	goto send_sigfin;
}

void ready_track01()
/* dosload_sigtmpÇÉZÉbÉgÉAÉbÉvÇµÇƒÇ®Ç≠Ç±Ç∆ */
/* ÉRÅ[ÉãíºëOÇÃÉXÉ^ÉbÉNÇ‡êÆóùÇµÇƒÇ®Ç≠Ç±Ç∆ */
{
	FDC_WORK *work == DS:0;

	POP(work->ret_EIP);
	// ÉgÉâÉbÉN0ÇÃÉçÅ[Éh
	EAX = work->track[0];
	TEST(AL, 0x01);
	if (== 0) {
		asmout("MOV EAX,OFFSET #pass00");
		work->dosload_phase = EAX;
		EAX = 0x0000; // C:0, H:0
		ECX = work->dosload_sigtmp;
		EDX = TRACK0_ADDR0;
pass98:
		FDC_MC_test4();
		return;
pass00:
		(char) work->track[0] |= 0x01;
		EAX = work->track[0];
	}

	// ÉgÉâÉbÉN1ÇÃÉçÅ[Éh
	TEST(AL, 0x02);
	if (== 0) {
		asmout("MOV EAX,OFFSET #pass01");
		work->dosload_phase = EAX;
		EAX = 0x0100; // C:0, H:1
		ECX = work->dosload_sigtmp;
		EDX = TRACK1_ADDR0;
		goto pass98;
pass01:
		(char) work->track[0] |= 0x02;
	//	EAX = work->track[0];
	}
pass99:
	JMP(work->ret_EIP);
}

void search_file()
/* ESÇîjâÛ */
{
	FDC_WORK *work == DS:0;

	// ÉtÉ@ÉCÉãÇÉTÅ[É`Ç∑ÇÈ
//	if ((unsigned) (char) work->dosload_name[0] == 0)
//		goto pass98;

	EAX = 1 * 8;
//	PUSH(ES);
	ES = EAX;
	EDI = ROOT_ENTRY0;
	do {
		EAX = [ES:EDI + 0];
		ECX = [ES:EDI + 4];
		EDX = [ES:EDI + 8];
		if (AL == 0)
			break;
		EDX &= 0x00ffffff;
		EAX -= work->dosload_name[0];
		ECX -= work->dosload_name[4];
		EDX -= work->dosload_name[8];
		EAX |= ECX;
		EAX |= EDX;
		if (== 0)
			goto find; /* CF = 0; */
		EDI += 32;
	} while ((unsigned) EDI < ROOT_ENTRY1);
//	POP(ES);
	// å©Ç¬Ç©ÇÁÇ»Ç©Ç¡ÇΩ
	STC();
find:
	return;
}

