		} while (AL != 0x80);
		EDX++;
		AL = [DS:EBX];
		(offset) cmd++;
		OUT(DX, AL);
		EDX--;
		ECX--;
	} while (!= 0);
	(offset) cmd++;

	// fdc_intÂæÖ„Å°
//	work->jmpvec_fdcint = EAX;
	asmout("MOV DWORD PTR DS:[0008H],OFFSET #fdcint");
	return;

fdcint:
	// „É™„Ç∂„É´„Éà„Çπ„ÉÜ„Éº„Çø„ÇπÂèó‰ø°
	(offset) status_buf = cmd[0];
	(offset) cmd += 4;
	cmd_free += 6;
	ECX = 7;
	EDX = fdc_status;
	do {
		do {
			// status„ÅÆbit6„Åå1„Åß„ÄÅbit7„Åå1„Å´„Å™„Çã„ÅÆ„ÇíÂæÖ„Å§„ÄÇ
			IN(AL, DX);
			AL &= 0xc0;
		} while (AL != 0xc0);
		EDX++;
		IN(AL, DX);
		EDX--;
		*status_buf = AL;
		(offset) status_buf++;
		ECX--;
	} while (!= 0);

	AL = 0x06; // ch2„ÅÆ„Éû„Çπ„ÇØ„Çí„Çª„ÉÉ„Éà
	OUT(DMA_singlemask, AL);

	asmout("JMP DWORD PTR DS:[EBX]");
#endif
}

void FDC_MC_WriteData()
{
	unsigned short DMA_address == 0x0009, DMA_counter == 0x000b, DMA_mode == 0x0017;
	unsigned short DMA_singlemask == 0x0015, DMA_addrhigh == 0x0023;

//	unsigned int fdc_control == 0x03f2; // bit7-4„ÅØmotorÂà∂Âæ°(1„ÅßON)„ÄÅbit3„ÅØDMAË®±ÂèØ„ÄÅbit2„ÅØFDC„É™„Çª„ÉÉ„Éà(0„Åß„É™„Çª„ÉÉ„Éà)„ÄÅbit1-0„ÅØ„Éâ„É©„Ç§„ÉñÁï™Âè∑
	unsigned int fdc_status == 0x0090, fdc_data == 0x0092;

	int *cmd == DS:EBX, cmd_free == EBP;
	char *status_buf == DS:EDI;

	// DMAË®≠ÂÆö
	AL = 0x4a; // sigle-mode, addr-inc., read, ch2
	OUT(DMA_mode, AL);
	EAX = cmd[4];
	OUT(DMA_counter, AL);
	SHR(EAX, 8);
	OUT(DMA_counter, AL);
	EAX = cmd[8];
	OUT(DMA_address, AL);
	SHR(EAX, 8);
	OUT(DMA_address, AL);
	SHR(EAX, 8);
	OUT(DMA_addrhigh, AL);
	AL = 0x02; // ch2„ÅÆ„Éû„Çπ„ÇØ„Çí„ÇØ„É™„Ç¢
	OUT(DMA_singlemask, AL);

	// „Ç≥„Éû„É≥„ÉâÈÄÅ‰ø°
	CL = [DS:EBX + 12];
	(offset) cmd += 13;

	do {
		IN(AL, fdc_status);
		AL &= CL;
	} while (!= 0);

	ECX = 7;
	do {
		do {
			// status„ÅÆbit6„Åå1„Åß„ÄÅbit7„Åå1„Å´„Å™„Çã„ÅÆ„ÇíÂæÖ„Å§„ÄÇ
			IN(AL, fdc_status);
			AL &= 0xc0;
		} while (AL != 0x80);
		AL = [DS:EBX];
		(offset) cmd++;
		OUT(fdc_data, AL);
		ECX--;
	} while (!= 0);

	do {
		IN(AL, fdc_status);
		AL &= 0xc0;
	} while (AL != 0x80);
	AL = 53; /* GPL „Éª„Éª„Éª ÂàÜ„Åã„Çâ„Å™„ÅÑ„ÅÆ„ÅßÈÅ©ÂΩì */
	OUT(fdc_data, AL);

	do {
		IN(AL, fdc_status);
		AL &= 0xc0;
	} while (AL != 0x80);
	AL = 0xff; /* DTL „Éª„Éª„Éª ÂàÜ„Åã„Çâ„Å™„ÅÑ„ÅÆ„ÅßÈÅ©ÂΩì */
	OUT(fdc_data, AL);

	// fdc_intÂæÖ„Å°
//	work->jmpvec_fdcint = EAX;
	asmout("MOV DWORD PTR DS:[0008H],OFFSET #fdcint");
	return;

fdcint:
	// „É™„Ç∂„É´„Éà„Çπ„ÉÜ„Éº„Çø„ÇπÂèó‰ø°
	(offset) status_buf = cmd[0];
	(offset) cmd += 4;
	cmd_free += 6;
	ECX = 7;
	do {
		do {
			// status„ÅÆbit6„Åå1„Åß„ÄÅbit7„Åå1„Å´„Å™„Çã„ÅÆ„ÇíÂæÖ„Å§„ÄÇ
			IN(AL, fdc_status);
			AL &= 0xc0;
		} while (AL != 0xc0);
		IN(AL, fdc_data);
		*status_buf = AL;
		(offset) status_buf++;
		ECX--;
	} while (!= 0);

	AL = 0x06; // ch2„ÅÆ„Éû„Çπ„ÇØ„Çí„Çª„ÉÉ„Éà
	OUT(DMA_singlemask, AL);

	asmout("JMP DWORD PTR DS:[EBX]");
}

