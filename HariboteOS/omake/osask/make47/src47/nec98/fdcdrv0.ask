	unsigned int init_sel     ==  2 * 8, syswork_sel   ==  3 * 8;
	unsigned int tapi_sel == 12 * 8;

	FDC_WORK *work == DS:0;

	// size0 : ñ¢ì]ëóÉoÉCÉgêî
	// size1 : sub-phase

	if (work->dosload_phase != 0) {
		INT(0x03);
	}

	asmout("FDC_writeIPL0: EQU $");

	// àÀóäÇ≥ÇÍÇΩílÇworkÇ…ï€ë∂ÇµÇƒÇ®Ç≠
	work->dosload_sigtmp = ECX;
	work->dosload_sigfin = EDX;

	/* [FS:EBX +  4] : boot-sector-code size */
	/* [FS:EBX +  8] : boot-sector-code addr */
	/* [FS:EBX + 12] : osask.exe size */
	/* [FS:EBX + 16] : osask.exe addr */

	PUSH(DS);
	PUSH(all_sel);
	POP(DS);
	PUSH(ESI);
	EAX = 0; /* Ç∆ÇËÇ†Ç¶Ç∏0ÉNÉäÉA */
	ESI = 0x104000;
	ECX = 512 * 1024 / 4; /* max:512KB */
	do {
		[DS:ESI] = EAX;
		ESI += 4;
		ECX--;
	} while (!= 0);
	EDX = [FS:EBX +  8];
	ESI = 0x100000;
	ECX = SECTOR_BYTES / 4;
	do {
		EAX = [DS:EDX];
		EDX += 4;
		[DS:ESI] = EAX;
		ESI += 4;
		ECX--;
	} while (!= 0);
	EDX = [FS:EBX + 16];
	ECX = [FS:EBX + 12];
	ESI = 0x104000;
	EDX += 512; // ÉwÉbÉ_Çì«Ç›îÚÇŒÇ∑
	ECX -= 512 - 3;
	(unsigned int) ECX /= 4;
	do {
		EAX = [DS:EDX];
		EDX += 4;
		[DS:ESI] = EAX;
		ESI += 4;
		ECX--;
	} while (!= 0);
