void FDC_MC_test4()
// EDX:„É≠„Éº„Éâ„Åô„ÇãÁâ©ÁêÜ„Ç¢„Éâ„É¨„Çπ, ECX:„Ç∑„Ç∞„Éä„É´Áï™Âè∑
// AH:sideÁï™Âè∑(0 or 1), AL:„Ç∑„É™„É≥„ÉÄ„ÉºÁï™Âè∑
// 1track„É≠„Éº„Éâ for DOS(64KBÂ¢ÉÁïå„Å´Ê≥®ÊÑè„Åó„Å¶„ÅÑ„Çã„ÅÆ„ÅßÂäπÁéá„Ç¢„ÉÉ„Éó)
// „Åì„ÅÆ„Éê„Éº„Ç∏„Éß„É≥„Åß„ÅØ„Ç∑„Éº„ÇØ„ÅÆÂøÖË¶Å„Åå„ÅÇ„Çã„Å®„Åç„Å†„Åë„Ç∑„Éº„ÇØ„Åô„Çã
{
	FDC_WORK *work == DS:0;
	int *cmd == DS:EBX, cmd_free == EBP;

	PUSHAD();
	MOV(ECX, CS);
	PUSH(DS);
	ECX += 8;
	DS = CX;
	PUSHFD();
	CLI();

	(offset) cmd = work->MC_ptr;
	if ((unsigned) (offset) cmd == 0x0100) {
		asmout("MOV DWORD PTR DS:[EBX+ 0],OFFSET FDC_MC_DriveReady");
		cmd[ 4] = 0;
		(offset) cmd += 8;
		CL = AH;
		AH <<= 2;
		if (AL != work->cylinder[0]) {
			asmout("MOV DWORD PTR DS:[EBX],OFFSET FDC_MC_SendCommand");
			cmd[ 4] = 2;
			cmd[ 8] = 0x00000711; // 11, 07 /* seek0 */, 00, 00
			if (AL != 0) {
				cmd[ 4] = 3;
				cmd[ 8] = 0x00000f11; // 11, 0f /* seek */, 00, 00
				[DS:EBX + 11] = AL; // C
				[DS:EBX + 10] = AH; // H
			}
			asmout("MOV DWORD PTR DS:[EBX+12],OFFSET FDC_MC_WaitFdcInt");
		//	asmout("MOV DWORD PTR DS:[EBX+16],OFFSET FDC_MC_SendCommand");
		//	cmd[20] = 1;
		//	cmd[24] = 0x00000810; // 10, 08 /* sense int status */, 00, 00
		//	asmout("MOV DWORD PTR DS:[EBX+28],OFFSET FDC_MC_ReadStatus");
		//	cmd[32] = 2;
		//	cmd[36] = 0x000000f8;
			(offset) cmd += 40-24;
			work->cylinder[0] = AL;
		}
		XCHG(AL, AH);
		asmout("MOV DWORD PTR DS:[EBX+ 0],OFFSET FDC_MC_ReadData");
		cmd[ 4] = 1024 * 8 - 1;
		cmd[ 8] = EDX;
		cmd[12] = 0x0000e611; // 11, e6 /* read data */, 00, 00
		cmd[16] = 0x08030100; // 00(H), 01(R), 03(N), 8(EOT)
		[DS:EBX + 14] = AX;
		[DS:EBX + 16] = CL;
		cmd[20] = 0x00f8;
		(offset) cmd += 24;

		EDI = EDX;
		EDI &= 0xffff0000;
		EDI += 0x00010000;
		EDI -= EDX;

		if ((unsigned) EDI < 8 * 1024) {
			EDI--;
			CH = 1;
			asmout("MOV DWORD PTR DS:[EBX+ 0],OFFSET FDC_MC_ReadData");
			cmd[ 4 - 24] = EDI;
			EDI++;
			EDI /= 4;
			ECX += EDI; /* CH„Å´Âä†ÁÆó */
			EDX += 8 * 1024;
			EDI = EDX;
			EDX &= 0xffff0000;
			EDI &= 0x0000ffff;
			EDI--;
			cmd[ 4] = EDI;
			cmd[ 8] = EDX;
			cmd[12] = 0x0000e611; // 11, e6 /* read data */, 00, 00
			cmd[16] = 0x08030100; // 00(H), 01(R), 03(N), 08(EOT)
			[DS:EBX + 14] = AX;
			[DS:EBX + 16] = CX;
			cmd[20] = 0x00f8;
			(offset) cmd += 24;
		}
		asmout("MOV DWORD PTR DS:[EBX+ 0],OFFSET FDC_MC_DriveOff");
		cmd[ 4] = 0;
		asmout("MOV DWORD PTR DS:[EBX+ 8],OFFSET FDC_MC_MakeSignal");
		cmd[12] = 2;
		EAX = work->TAPI_sendsignalofs;
		ECX = work->TAPI_sendsignalsel;
		cmd[16] = EAX;
		cmd[20] = ECX;
		EAX = [SS:ESP + 32];
		cmd[24] = 0x1241;
		cmd[28] = EAX;
		asmout("MOV DWORD PTR DS:[EBX+32],OFFSET FDC_MC_Finish");

		(offset) cmd = 0x100;
		asmout("CALL DWORD PTR DS:[EBX]");
		work->MC_ptr = (offset) cmd;
		work->MC_free = cmd_free;
	}

	POPFD(); // 0

	POP(DS); // 4
	POPAD(); // 8
	return;
}

