void FDC_MC_SendCommand()
//	„Ç≥„Éû„É≥„Éâofs
//	„É©„Ç§„Éà„Åô„Çã„Éê„Ç§„ÉàÊï∞
//	busy„ÉÅ„Çß„ÉÉ„ÇØ„Éû„Çπ„ÇØ„Éê„Ç§„Éà„ÄÅ„Ç≥„Éû„É≥„Éâ
{
//	unsigned int fdc_control == 0x03f2; // bit7-4„ÅØmotorÂà∂Âæ°(1„ÅßON)„ÄÅbit3„ÅØDMAË®±ÂèØ„ÄÅbit2„ÅØFDC„É™„Çª„ÉÉ„Éà(0„Åß„É™„Çª„ÉÉ„Éà)„ÄÅbit1-0„ÅØ„Éâ„É©„Ç§„ÉñÁï™Âè∑
	unsigned int fdc_status == 0x0090, fdc_data == 0x0092;

	int *cmd == DS:EBX, cmd_free == EBP;
	char *status_buf == DS:EDI;

	EAX = cmd[4];
	// „Åì„Çå„Å´1„ÇíÂä†„Åà„ÄÅ„Åù„ÅÆÂæå„Å´3„ÇíÂä†„Åà„ÄÅ4„ÅßÂâ≤„Çã
	(offset) cmd += 9;
	SHR(EAX, 2);
	/* cmd_free += EAX + 3 */ asmout("LEA EBP,[EBP+EAX+3]");
	do {
		IN(AL, fdc_status);
		AL &= [DS:EBX - 1];
	} while (!= 0);
	ECX = cmd[4 - 9];
	do {
		do {
			//	status„ÅÆbit6„Åå1„Åß„ÄÅbit7„Åå1„Å´„Å™„Çã„ÅÆ„ÇíÂæÖ„Å§„ÄÇ
			IN(AL, fdc_status);
			AL &= 0xc0;
		} while (AL != 0x80);
		AL = [DS:EBX];
		(offset) cmd++;
		OUT(fdc_data, AL);
		ECX--;
	} while (!= 0);
	(offset) cmd += 3;
	(offset) cmd &= 0xfffffffc;
	asmout("JMP DWORD PTR DS:[EBX]");
}

void far FDC_int()
{
	FDC_WORK *work == DS:0;
	int *cmd == DS:EBX, cmd_free == EBP;
	unsigned short PIC_mastar_OCW2 == 0x0000;
	unsigned short PIC_slave_OCW2 == 0x0008, PIC_slave_ISR == 0x0008, PIC_slave_OCW3 == 0x0008;

	PUSHAD();
	IN(AL, 0x0090); /* Ââ≤„ÇäËæº„Åø„ÇíÁµÇ‰∫Ü„Åï„Åõ„Çã„Åü„ÇÅ„Å´„ÄÅ„Çπ„ÉÜ„Éº„Çø„Çπ„ÇíË™≠„ÇÄ */
	DL = AL;
	MOV(ECX, CS);
	PUSH(DS);
	ECX += 8;
	asmout("INC BYTE PTR SS:[0FFFFFFE0H]");
	DS = CX;	
	asmout("INC BYTE PTR SS:[0FFFFFFF0H]");

	AL = 0x63 /* IRQ-11„ÇíÁµÇ‰∫Ü */;
	OUT(PIC_slave_OCW2, AL);
	AL = 0x4b; /* ISRË™≠„ÅøËæº„Åø */
	OUT(PIC_slave_OCW3, AL);
	NOP();
	IN(AL, PIC_slave_ISR);
	if (AL == 0) {
		AL = 0x67 /* IRQ-7„ÇíÁµÇ‰∫Ü */;
		OUT(PIC_mastar_OCW2, AL);
	}

	/* EAX„ÅÆÂÄ§„Åß„ÅØ„Å™„Åè„ÄÅDL & 0xc0„ÅÆÂÄ§„Å´„Çà„Å£„Å¶ÊåØ„ÇäÂàÜ„Åë„ÅüÊñπ„Åå„ÅÑ„ÅÑ„Åã„ÇÇ„Åó„Çå„Å™„ÅÑ ‚Üí „Åù„ÅÆ„Å®„Åä„ÇäÔºÅ */
	/* 0x80„Å™„Çâ„Çª„É≥„Çπ„Åô„Çã */

	// FDC_MC„ÇíÂÆüË°å‰∏≠„ÅØ„ÄÅCLI();„Åó„Å¶„Åä„Åè„Åì„Å®
	TEST(DL, 0x50);
	if (== 0) {
		do {
			//	status„ÅÆbit6„Åå1„Åß„ÄÅbit7„Åå1„Å´„Å™„Çã„ÅÆ„ÇíÂæÖ„Å§„ÄÇ
			IN(AL, 0x0090);
			AL &= 0xc0;
		} while (AL != 0x80);
		AL = 0x08;
		OUT(0x0092, AL);
		NOP();
		do {
			//	status„ÅÆbit6„Åå1„Åß„ÄÅbit7„Åå1„Å´„Å™„Çã„ÅÆ„ÇíÂæÖ„Å§„ÄÇ
			IN(AL, 0x0090);
			AL &= 0xc0;
		} while (AL != 0xc0);
		IN(AL, 0x0092); /* ST0 */
		DL = AL;
		do {
			//	status„ÅÆbit6„Åå1„Åß„ÄÅbit7„Åå1„Å´„Å™„Çã„ÅÆ„ÇíÂæÖ„Å§„ÄÇ
			IN(AL, 0x0090);
			AL &= 0xc0;
		} while (AL != 0xc0);
		IN(AL, 0x0092);
		TEST(DL, 0x80);
		if (!= 0)
			goto skip; /* Áä∂ÊÖãÈÅ∑Áßª„Å†„Å£„Åü„ÅÆ„Åß„ÄÅÁÑ°Ë¶ñ */
	}

	(offset) cmd = work->MC_ptr;
	cmd_free = work->MC_free;

	CALL(work->jmpvec_fdcint);
	// Áä∂ÊÖãÈÅ∑ÁßªÂâ≤„ÇäËæº„Åø„Åå„ÅÇ„Çã„Å™„Çâ„ÄÅjmpvec_fdcint„ÇíÊõ∏„ÅçÊèõ„Åà„Å¶„Åä„Åë„ÄÇ„Åù„ÅÆÈöõ„ÄÅcmd„Å®cmd_free„ÅØ‰øùÂ≠ò„Åô„Çã„ÄÇ

	work->MC_ptr = (offset) cmd;
	work->MC_free = cmd_free;
skip:
	asmout("DEC BYTE PTR SS:[0FFFFFFF0H]");
	if (== 0) {
		asmout("CALL FAR DWORD SS:[0FFFFFFF8H]");
	}
	POP(DS);
	POPAD();
	asmout("DEC BYTE PTR SS:[0FFFFFFE0H]");
	if (== 0) {
		jmp_system_count0();
	}
	IRETD();
}

