	work->timer_cmd5 = 0;
	work->timer_cmd6 = 5 * 4 /* „Çø„Ç§„Éû„ÉºÂèñ„ÇäÊ∂à„Åó */;
	EAX = 0;
	MOV(ECX, SS);
