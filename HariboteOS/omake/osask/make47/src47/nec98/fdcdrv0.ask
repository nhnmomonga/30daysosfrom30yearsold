// GS:ESI ÉtÉ@ÉCÉãñº(11bytes)
{
	unsigned int tapi_sel == 12 * 8;
	FDC_WORK *work == DS:0;

	if (work->dosload_phase != 0) {
		INT(0x03);
	}

	// àÀóäÇ≥ÇÍÇΩílÇworkÇ…ï€ë∂ÇµÇƒÇ®Ç≠
	work->dosload_addr = EAX;
	save_dosname();

	// ÉtÉ@ÉCÉãÇÉTÅ[É`Ç∑ÇÈ

	PUSH(ES);
	search_file();
	if (CF != 0)
		INT(0x03); /* å©ïtÇ©ÇÁÇ»Ç©Ç¡ÇΩ */
	EAX = (unsigned short) [ES:EDI + 0x1a];
	ECX = [ES:EDI + 0x1c];
	POP(ES);
	work->dosload_FAT = EAX;
	work->dosload_size0 = ECX;
	work->dosload_size1 = ECX;

	// ÉçÅ[ÉfÉBÉìÉOÉãÅ[Év
	if (ECX != 0) {
		for (;;) {
			// FATî‘çÜÇ©ÇÁÉgÉâÉbÉNî‘çÜÇãÅÇﬂÇÈ
		//	EAX = work->dosload_FAT;
			EAX += SYSTEM_SECTORS; // (1.44MB:+31, 1.2MB:+9)
			/* 1.44MBÇ≈ÇÕ18Ç≈äÑÇÈÅA1.2MBÇ≈ÇÕ8Ç≈äÑÇÈ */
		//	CL = 18; /* for 1.44MB */
			EDX = EAX; // ï®óùÉAÉhÉåÉXEDX
		//	DIV(CL); // ALÇ…ÉgÉâÉbÉNî‘çÜ /* for 1.44MB */
		//	EAX &= 0xff; /* for 1.44MB */
			EAX /= 8; /* for 1.2MB */

#if 0
			/* track cache check */
			BT(work->track[0], EAX);
			if (CF == 0) {
			//	EDX = EAX * 9;
			//	LEA(EDX, [EAX * 8 + EAX]);
				EDX = EAX;
				asmout("MOV ECX,OFFSET #pass02");
			//	EDX *= 1024;
				EDX *= 8192;
				work->dosload_phase = ECX;
				(unsigned char) AL >>= 1;
				ECX = work->dosload_sigtmp;
				ADC(AH, 0);
				EDX += 0x100000;
				FDC_MC_test4();
				goto pass99;
pass02:
				// FATî‘çÜÇ©ÇÁÉgÉâÉbÉNî‘çÜÇãÅÇﬂÇÈ
				EAX = work->dosload_FAT;
				EAX += SYSTEM_SECTORS; // (1.44MBÇ≈ÇÕ+31ÅA1.2MBÇ≈ÇÕÅA+9)
			//	CL = 8;
				EDX = EAX; // ï®óùÉAÉhÉåÉXEDX
			//	DIV(CL); // ALÇ…ÉgÉâÉbÉNî‘çÜ
			//	EAX &= 0xff;
				EAX /= 8;

				BTS(work->track[0], EAX);
			}
#endif

			PUSH(EDI);
			PUSH(ESI);
			PUSH(DS);
			EAX = 1 * 8;
			EDX *= SECTOR_BYTES;
			ECX = work->dosload_size1;
			EDX += TRACK0_ADDR0;
			ECX -= SECTOR_BYTES;
			EDI = work->dosload_addr;
			if ((unsigned) <= 0)
				break;
			PUSH(work->dosload_FAT);
			work->dosload_size1 = ECX;
			DS = AX;
			ESI = 0;
			ECX = SECTOR_BYTES / 4;
			do {
				EAX = [DS:EDI];
				EDI += 4;
				EBX = [DS:EDX];
				[DS:EDX] = EAX;
				EAX ^= EBX;
				EDX += 4;
				ESI |= EAX;
				ECX--;
			} while (!= 0);

			/* éüÇÃFATî‘çÜÇéÊìæ */
			POP(EAX);
			ECX = EAX;
			EBX = EAX;
			(unsigned int) EAX >>= 1;
			LEA(EBX, [EBX + EAX]);
			AL = [DS:EBX + FAT0_ADDR0 + 0];
			AH = [DS:EBX + FAT0_ADDR0 + 1];
			if (CF == 1)
				EAX >>= 4;
			EAX &= 0x0fff;

			POP(DS);
			if (ESI != 0) {
				ECX += SYSTEM_SECTORS;
				BTS(work->WBFS[0], ECX);
			}
			POP(ESI);
			POP(EDI);

			work->dosload_addr += SECTOR_BYTES;
			work->dosload_FAT = EAX;
		}
		ECX += SECTOR_BYTES;
		if (!= 0) {
			DS = EAX;
			EAX = 0;
			ESI = 0;
			do {
				AL = [DS:EDI];
				EDI++;
				BL = [DS:EDX];
				[DS:EDX] = AL;
				AL ^= BL;
				EDX++;
				ESI |= EAX;
				ECX--;
			} while (!= 0);
		}
		POP(DS);
		if (ESI != 0) {
			EAX = work->dosload_FAT;
			EAX += SYSTEM_SECTORS;
			BTS(work->WBFS[0], EAX);
		}
		POP(ESI);
		POP(EDI);
	}

	work->dosload_phase = 0;
}

void check_WBF()
{
	unsigned int tapi_sel == 12 * 8;
	FDC_WORK *work == DS:0;

	if (work->WBF[0] == 0) {
		ECX = 360;
		EAX = 0;
		do {
		//	EAX |= work->WBFS[ECX - 4];
			EAX |= [ECX - 4 + work->WBFS];
			ECX -= 4;
		} while (!= 0);
		if (EAX != 0) {
			work->WBF[0] = 1;
			MOV(EAX, SS);
			PUSH(FS);
			PUSH((int) 0); // eoc
			PUSH((int) 0); /* dummy */
			PUSH(0x0090 /* SIGNAL_NEED_WB */);
			PUSH(0x7f000002);
			PUSH(0x4243);
			PUSH(0x0128 /* ÉRÉ}ÉìÉhÉVÉOÉiÉãíºê⁄(with wait) */);
			FS = EAX;
			EBX = ESP;
			DB(0x9a); DD(0); DW((short) tapi_sel);
			ESP += 6 * 4;
			POP(FS);
		}
	}
	return;
}

void FDC_deletefile()
// ECX:çÏã∆íÜÇÃÉVÉOÉiÉã
// EDX:äÆóπÇÃÉVÉOÉiÉã
// GS:ESI ÉtÉ@ÉCÉãñº(11bytes)
{
